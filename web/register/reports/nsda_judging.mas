<%args>
	$tourn
	$tourn_settings
</%args>
<%init>

	$m->clear_buffer();
	$r->content_type('application/json');

	foreach my $school ($tourn->schools()) { 

		$m->comp("/funclib/judgemath/nats_check_judging.mas",
			school         => $school,
			tourn          => $tourn,
			tourn_settings => $tourn_settings
		);

	}

	$m->print('{ 
		"error": false, 
		"message": "Judging check complete"
	}');

	$m->abort();

</%init>
