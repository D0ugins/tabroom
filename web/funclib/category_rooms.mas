<%args>
	$category
</%args>
<%init>

	Tab::Room->set_sql( by_category => "
		select distinct room.* 
		from room, round, event, panel
		where panel.room = room.id
		and panel.round = round.id
		and round.event = event.id
		and event.category = ? 
		order by room.name
	");

	return Tab::Room->search_by_category($category->id);

</%init>
