<%init>

	use Tab::NSDA::NationalsDBI;

	my $dbh = Tab::NSDA::NationalsDBI->db_Main();

	my $sth = $dbh->prepare("
		select district_id, code
		from nationals.TRPC_DISTRICT_CODES
	");

	$sth->execute();

	my %districts;

	while ( my ($district, $state) = $sth->fetchrow_array() ) { 
		$districts{$district} = $state;
	}

	return %districts;

</%init>
