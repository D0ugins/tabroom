<%args>
	$school
</%args>
<%init>

	Tab::Judge->columns(TEMP => "reason");

	Tab::Judge->set_sql(incomplete => "
		select judge.*, judge_setting.value_text as reason
			from judge, judge_setting
		where judge.school = ?
			and judge.id = judge_setting.judge
			and judge_setting.tag = 'incomplete'
			and judge.active = 1
	");

	return Tab::Judge->search_incomplete($school->id);

</%init>
