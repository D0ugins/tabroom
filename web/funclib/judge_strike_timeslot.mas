<%args>
	$school
	$category
	$strike_timeslot
</%args>
<%init>

	Tab::Judge->set_sql( stimes => "
		select distinct judge.* from judge
		where judge.school = ?
		and category = ? 
		and exists ( select strike.id from strike
			where strike.judge = judge.id
			and strike.strike_timeslot = ? )
	");

	return Tab::Judge->search_stimes( $school->id, $category->id, $strike_timeslot->id );


</%init>
