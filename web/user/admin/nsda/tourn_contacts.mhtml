<%args>
	$tourn_id
</%args>
<%init>

	my $msg;

	if ($tourn_id) { 

		my $tourn = Tab::Tourn->retrieve($tourn_id);

		foreach my $tag (1 .. 3) { 
			$tourn->setting("nsda_contact_".$tag, $ARGS{"nsda_contact_".$tag});
		}

		$msg = "NSDA Office Contacts Saved for ".$tourn->name;

	}

	$m->redirect("tourns.mhtml?msg=$msg");


</%init>
