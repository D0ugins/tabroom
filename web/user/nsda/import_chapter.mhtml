<%args>
	$person
	$session
	$nsda_school_id
	$foreign_chapter_id => undef
</%args>
<%init>

	use Tab::NSDA::MemberSchool;

	my $nsda_school = Tab::NSDA::MemberSchool->retrieve($nsda_school_id);

	my $chapter = $m->comp(
		"/funclib/chapter_import.mas",
		nsda_school => $nsda_school,
		nome        => 1,
	);

	my $msg = "School ".$chapter->name." has been imported into Tabroom for Nationals!";

	$m->redirect("/user/nsda/district.mhtml?district_id=".$chapter->district->id."&msg=$msg");

</%init>

