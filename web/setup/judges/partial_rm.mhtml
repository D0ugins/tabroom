<%args>
	$tourn
	$partial_id
</%args>
<%perl>

	my $bin = Tab::StrikeTime->retrieve($partial_id);

	$m->abort unless $bin && $bin->category;

	my $category_id = $bin->category->id;

	my $msg = "Partial timeblock ".$bin->name." deleted";

	foreach my $strike ($bin->strikes) { 
		$strike->delete;
	}

	$bin->delete;

	$m->redirect("$Tab::url_prefix/setup/judges/partials.mhtml?category_id=$category_id&msg=$msg");


</%perl>

