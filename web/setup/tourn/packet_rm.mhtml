<%args>
	$tourn
	$person
	$tourn_settings
</%args>
<%init>

	my $description = "Registration packet deleted";

	$tourn->setting("registration_packet", 0);
	my $now = DateTime->now();


	Tab::ChangeLog->create({
		type        => 'tabbing',
		tourn       => $tourn->id,
		person      => $person->id,
		description => $description,
		created     => $now
	});

	my $msg = "Tournament packet deleted";

	$m->redirect("settings.mhtml?msg=$msg");

</%init>

