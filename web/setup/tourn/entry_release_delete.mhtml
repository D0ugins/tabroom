<%args>
	$person
	$tourn
</%args>
<%init>

	my $filename  = $tourn->setting("entry_release_form");
	my $filepath = "$Tab::file_root/files/tourns/".$tourn->id."/entry_release/$filename" if $filename;
	my $garbage = `rm -f $filepath`;
	$tourn->setting("entry_release_form", 0);

	system "$Tab::s3_cmd del $Tab::s3_bucket/tourns/$tourn/$filename";

	my $msg = "Invite deleted";
	$m->redirect("main.mhtml?msg=$msg");

</%init>
