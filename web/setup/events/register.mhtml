				<script>

					function waitlistExists() {

						var exists;

						if (
							$("#school_cap").val() > 0
							|| $("#total_cap").val() > 0
						) {
							$(".nocap_options").addClass('hidden');
						} else {
							$(".nocap_options").removeClass('hidden');
						}

						if (
							$("#school_cap").val() > 0
							|| $("#total_cap").val() > 0
							|| $("#waitlist").prop("checked")
						) {
							$(".waitlist_options").removeClass('hidden');
						} else {
							$(".waitlist_options").addClass('hidden');
						}

						zebraRows();
					}

					$(document).ready(function() {
						waitlistExists();
					});
