<%args>
	$person
	$tourn
	$round_id => undef
</%args>
<%init>

	my $round = Tab::Round->retrieve($round_id);

	# Can't call the download round script directly because without objects it
	# would require the user's password.

	$m->comp("/api/download_round.mhtml", 
		round  => $round,
		tourn  => $tourn,
		person => $person 
	);

</%init>
