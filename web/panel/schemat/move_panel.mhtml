<%args>
	$person
	$panel_id => undef
	$round_id => undef
</%args>
<%init>

	my $msg;

	my $panel = Tab::Panel->retrieve($panel_id) if $panel_id;
	my $round = Tab::Round->retrieve($round_id) if $round_id;

	if ($panel 
		&& $round 
		&& $panel->round->event == $round->event
	) {

		$panel->round($round->id);
		$panel->update();

	}

	$m->redirect("panel_view.mhtml?panel_id=<% $panel->id %>");


</%init>

