<%args>
	$round_id
	$tourn
	$tourn_settings
</%args>
<%init>

	my $round = Tab::Round->retrieve($round_id);

	my $hello = $m->comp('/funclib/online_usage.mas',
		tourn          => $tourn,
		tourn_settings => $tourn_settings,
		round          => $round,
		debug          => 1
	);

</%init>

	<div class="main">

		<p><% $round->realname %></p>

		<p>Output:</p>

		<p><% $hello ? $hello : "You are fine!  Round will publish"%></p>

	</div>

