<%args>
	$setting_id
</%args>
<%init> 

	my $setting = Tab::RoundSetting->retrieve($setting_id);

	my $round_id;
	my $msg;

	if ($setting) { 
		$round_id = $setting->round->id if $setting->round;
		$setting->delete;
		$msg = "Scheduled publication cancelled";
	}

	$m->redirect("blast.mhtml?round_id=".$round_id."&msg=$msg");

</%init>


