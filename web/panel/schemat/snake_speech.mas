							}

							if ($speaker == $last_order) {
								$counts{"last_order"}++
							} else {
								$counts{"order"} += $orders{$speaker};
							}

							my $crosses_clash;

							if ($crosses_entries{$entry_id}) {
								foreach my $other (keys %{$crosses_entries{$entry_id}}) {
									if (
										(abs $crosses_entries{$entry_id}{$other}{"order"} - $speaker)
										< 2
									) {
										$counts{"crosses"}++;
										$crosses_clash++;
									}
								}
							}
</%perl>

							<span
								class="
									marno
									entries
									draggable
									centeralign
									<% $last_order == $speaker ? "order_last" : "" %>
