<%args>

</%args>
<%init>

	my $tourn = Tab::Tourn->retrieve(23101);
	my $count;

	foreach  my $category ($tourn->categories) {
		foreach my $judge ($category->judges) { 
			$m->comp("/funclib/nsda/judge_nats_check.mas", judge => $judge);
			$count++;

			unless ($count % 100) { 
				$m->print("<p>Did $count judges</p>");
				$m->flush_buffer();
			}
		}

	}


</%init>
