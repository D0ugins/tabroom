<%init>

	my $circuit = Tab::Circuit->retrieve(3);

	$m->clear_buffer();

	foreach my $diocese ($circuit->regions) {
		$diocese->setting("cooke", $diocese->cooke_pts);
		$diocese->setting("arch", $diocese->arch);
		$diocese->setting("quota", $diocese->quota);

		$m->print("<p>Did ".$diocese->name."</p>");
		$m->flush_buffer();
	}

	$m->abort();

</%init>
