{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@speechanddebate/eslint-config-nsda": "^1.0.15",
    "@speechanddebate/nsda-js-utils": "^1.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "js-cookie": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.2",
    "react-scripts": "^5.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "stacktrace-js": "^2.0.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --ext js src",
    "test": "react-scripts test",
    "test-silent": "react-scripts test --silent",
    "test-cover": "react-scripts test --coverage --watchAll",
    "test-ci": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "@speechanddebate/eslint-config-nsda",
      "@speechanddebate/eslint-config-nsda/react"
    ],
    "rules": {
      "no-tabs": 0,
      "key-spacing": 0,
      "comma-spacing": 0,
      "implicit-arrow-linebreak": 0,
      "space-before-function-paren": 0,
      "space-in-parens": 0,
      "import/no-dynamic-require": 0,
      "global-require": 0,
      "padded-blocks": 0,
      "no-plusplus": 0,
      "indent": ["error", "tab"],
      "no-multi-spaces": [0, { "exceptions": { "Property": true, "VariableDeclarator": true } }],
      "react/jsx-indent": [0, "tab", { "checkAttributes": false, "indentLogicalExpressions": true }],
      "react/jsx-indent-props": 0,
      "react/jsx-equals-spacing": 0,
      "jsx-a11y/anchor-has-content": 0
    },
    "overrides": [
      {
        "files": [
          "**/*.js?(x)"
        ]
      },
      {
        "files": [
          "**/*.test.js"
        ],
        "plugins": [
          "jest"
        ],
        "extends": [
          "plugin:jest/recommended"
        ],
        "rules": {
          "jest/prefer-expect-assertions": "off",
          "jest/expect-expect": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint-plugin-jest": "^26.5.3"
  },
  "jest": {
    "resetMocks": false,
    "transformIgnorePatterns": []
  }
}
